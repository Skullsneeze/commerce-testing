{
  "name": "commerce-testing",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdobeDocs/commerce-testing"
  },
  "dependencies": {
    "@devdocs/gatsby-theme-acd": "0.0.2",
    "gatsby": "4.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "remark-cli": "^11.0.0",
    "remark-validate-links": "^12.1.0"
  },
  "scripts": {
    "start": "gatsby build && gatsby serve",
    "start:prefix": "gatsby build --prefix-paths && gatsby serve --prefix-paths",
    "dev": "gatsby develop --open",
    "dev:https": "gatsby develop --https --host localhost.corp.adobe.com --port 9000",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:links": "remark src/pages --quiet --frail"
  },
  "remarkConfig": {
    "plugins": [
      "remark-validate-links"
    ]
  }
}
